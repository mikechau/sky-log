{
  "name": "sky-log",
  "version": "0.0.0",
  "description": "Air travel itinerary, built with React.",
  "scripts": {
    "packages:reinstall": "rm -rf node_modules && npm install",
    "packages:updates": "npm-check-updates",
    "packages:upgrade": "npm-check-updates -u",
    "shrinkwrap:build": "npm-shrinkwrap --dev",
    "shrinkwrap:remove": "rm npm-shrinkwrap.json",
    "shrinkwrap:rebuild": "npm run shrinkwrap:remove && npm run packages:reinstall && npm run shrinkwrap:build",
    "test": "mocha ./test --recursive --reporter spec",
    "test:watch": "mocha ./test --recursive --reporter spec -w",
    "test:watch:browser": "mocha ./test --recursive --reporter spec -w 2>&1 | report-viewer",
    "server:start": "node server.js"
  },
  "keywords": [
    "react",
    "travel"
  ],
  "author": "Michael Chau @mikechau",
  "license": "MIT",
  "dependencies": {
    "alertifyjs": "^1.4.1",
    "alt": "^0.16.10",
    "classnames": "^2.1.2",
    "humps": "^0.5.2",
    "lodash": "^3.9.3",
    "moment": "^2.10.3",
    "normalizr": "^0.1.3",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.2",
    "react-json-inspector": "^5.0.3",
    "react-router": "^0.13.3",
    "react-router-bootstrap": "^0.15.0"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-core": "^5.5.6",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.5.6",
    "chai": "^3.0.0",
    "compression-webpack-plugin": "^0.2.0",
    "css-loader": "^0.14.5",
    "eslint": "^0.22.1",
    "eslint-loader": "^0.12.0",
    "eslint-plugin-react": "^2.5.0",
    "expose-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.5.0",
    "jsdom": "^5.4.3",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^0.4.0",
    "mockery": "^1.4.0",
    "nock": "^2.4.1",
    "null-loader": "^0.1.1",
    "promise-loader": "^0.1.3",
    "react-hot-loader": "^1.2.7",
    "react-tools": "^0.13.3",
    "report-viewer": "^0.3.1",
    "sinon": "^1.15.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "engine": "node >= 0.10.x"
}
